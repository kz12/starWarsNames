{
  "name": "star_wars_names_kai",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "cover": "nyc npm t",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:main build:umd build:umd.min",
    "build:main": "babel --copy-files --out-dir dist --ignore src/*.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kz12/starWarsNames.git"
  },
  "keywords": [
    "star",
    "wars",
    "kai"
  ],
  "author": "Kai Zhong <kz12@ic.ac.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kz12/starWarsNames/issues"
  },
  "homepage": "https://github.com/kz12/starWarsNames#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "nyc": "10.1.2",
    "rimraf": "2.5.4",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ]
}
