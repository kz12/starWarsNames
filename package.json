{
  "name": "star_wars_names",
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:single": "istanbul cover -x src/*.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kz12/starWarsNames.git"
  },
  "keywords": [
    "star",
    "wars"
  ],
  "author": "Kai Zhong <kz12@ic.ac.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kz12/starWarsNames/issues"
  },
  "homepage": "https://github.com/kz12/starWarsNames#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  }
}
